<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon
  id="repository.bbelectric"
  name="bbelectric Add-ons"
  version="1.0.0"
  provider-name="bbelectric">
  <requires>
    <import addon="xbmc.addon"
            version="13.0.0"/>
  </requires>
  <extension point="xbmc.addon.repository"
             name="Official bbelectric's Add-on Repository">
    <info compressed="true">http://raw.github.com/bbelectric/xbmc.repo.bbelectric/master/addons.xml</info>
    <checksum>http://raw.github.com/bbelectric/xbmc.repo.bbelectric/master/addons.xml.md5</checksum>
    <datadir zip="true">http://raw.github.com/bbelectric/xbmc.repo.bbelectric/master/download</datadir>
    <hashes>false</hashes>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Install Add-ons from bbelectric's repository</summary>
    <description lang="en">Download and install add-ons from the official bbelectric's addon repository.</description>
    <platform>all</platform>
    <nofanart>true</nofanart>
  </extension>
</addon>

<addon id="script.absolutetv.updater" name="AbsoluteTV Updater" version="2.1.3" provider-name="bbelectric">
    <requires>
        <import addon="xbmc.python" version="2.14.0"/>
        <import addon="xbmc.addon" version="12.0.0"/>
    </requires>
    <extension point="xbmc.python.script" library="default.py">
        <provides>executable</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <language></language>
        <platform>all</platform>
    </extension>
</addon>
</addons>
